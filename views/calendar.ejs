                        </tr>
                        <% } %>
                        <% }); %>
                        </tbody>
                    </table>
                </ul>
            </div>

            <div class="dropdown" id="dep1">
                <button class="btn btn-block dropdown-toggle" type="button" data-toggle="dropdown">Material Science & Engineering
                    <span class="caret"></span></button>
                <ul class="dropdown-menu">
                    <table class="table table-striped">
                        <thead>
                        <tr>
                            <th>Module</th>
                            <th>Date</th>
                            <th>Time</th>
                            <th>Venue</th>
                        </tr>
                        </thead>
                        <tbody>
                        <% exams.forEach(function (exam) { %>

                        <%if (exam.department=='MT') { %>
