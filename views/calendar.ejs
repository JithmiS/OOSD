                        <% }); %>
                        </tbody>
                    </table>
                </ul>
            </div>

            <div class="dropdown" id="dep1">
                <button class="btn btn-block dropdown-toggle" type="button" data-toggle="dropdown">Civil Engineering
                    <span class="caret"></span></button>
                <ul class="dropdown-menu">
                    <table class="table table-striped">
                        <thead>
                        <tr>
                            <th>Module</th>
